{
  "manifest_version": 2,

  "name": "Codeforces problem Tag hide",
  "description": "This extension is used to hide tags.",
  "version": "1.0",

  "browser_action": {
    "default_icon": "icon.png",
    "default_popup": "popup.html"
  },
    "content_scripts": [
        {
            "matches": ["http://codeforces.com/contest/*"],
            "js" : ["jquery-2.1.1.min.js", "content.js"]
        }
    ]
}
